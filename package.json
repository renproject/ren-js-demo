{
    "name": "ren-js-example",
    "version": "0.1.0",
    "private": true,
    "homepage": "https://renproject.github.io/ren-js-demo-mainnet",
    "scripts": {
        "start": "react-scripts start",
        "clean": "yarn rimraf ./build",
        "build": "NODE_OPTIONS=--max_old_space_size=4096 REACT_APP_SOURCE_VERSION=$SOURCE_VERSION react-scripts build",
        "test": "react-scripts test",
        "eject": "react-scripts eject",
        "predeploy": "$npm_execpath run build",
        "deploy": "gh-pages -d build --repo git@github.com:renproject/ren-js-demo-mainnet.git",
        "link:all": "yarn link @renproject/rpc @renproject/interfaces @renproject/utils @renproject/chains-ethereum @renproject/chains-filecoin @renproject/ren",
        "unlink:all": "yarn unlink @renproject/rpc @renproject/interfaces @renproject/utils @renproject/chains-ethereum @renproject/chains-filecoin @renproject/ren @renproject/chains-bitcoin"
    },
    "dependencies": {
        "@material-ui/core": "^4.11.0",
        "@material-ui/icons": "^4.9.1",
        "@renproject/chains-bitcoin": "^2.0.0-alpha.22",
        "@renproject/chains-ethereum": "^2.0.0-alpha.22",
        "@renproject/chains-filecoin": "^2.0.0-alpha.22",
        "@renproject/interfaces": "^2.0.0-alpha.22",
        "@renproject/multiwallet-binancesmartchain-injected-connector": "^2.0.0-alpha.22",
        "@renproject/multiwallet-ethereum-injected-connector": "^2.0.0-alpha.22",
        "@renproject/multiwallet-ethereum-walletconnect-connector": "^2.0.0-alpha.22",
        "@renproject/multiwallet-ui": "^2.0.0-alpha.22",
        "@renproject/react-components": "^2.0.0-alpha.16",
        "@renproject/ren": "^2.0.0-alpha.22",
        "@renproject/rpc": "^2.0.0-alpha.22",
        "@renproject/utils": "^2.0.0-alpha.22",
        "@types/classnames": "^2.2.11",
        "@types/node": "^14.14.7",
        "@types/qs": "^6.9.5",
        "@types/react": "^16.9.34",
        "@types/react-dom": "^16.9.6",
        "@types/react-router-dom": "^5.1.4",
        "@walletconnect/web3-provider": "^1.3.1",
        "bignumber.js": "^9.0.1",
        "classnames": "^2.2.6",
        "immutable": "^4.0.0-rc.12",
        "qs": "^6.9.4",
        "react": "17.0.1",
        "react-copy-to-clipboard": "^5.0.2",
        "react-dom": "17.0.1",
        "react-router-dom": "^5.2.0",
        "web3": "^2.0.0-alpha.1"
    },
    "devDependencies": {
        "@types/react": "^16.9.56",
        "@types/react-dom": "^16.9.9",
        "@types/react-router": "^5.1.8",
        "@types/react-router-dom": "^5.1.6",
        "gh-pages": "^3.1.0",
        "jest": "26.6.0",
        "node-sass": "^4.14.1",
        "react-scripts": "^4.0.0",
        "tslint": "^6.1.3",
        "typescript": "^4.0.5"
    },
    "resolutions": {},
    "browserslist": [
        ">0.2%",
        "not dead",
        "not ie <= 11",
        "not op_mini all"
    ],
    "prettier": {
        "tabWidth": 4
    },
    "eslintConfig": {
        "extends": [
            "react-app"
        ]
    }
}
